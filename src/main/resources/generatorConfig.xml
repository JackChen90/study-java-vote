<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <!-- 具体配置内容 -->
    <!--jdbc驱动-->
    <classPathEntry
            location="/Users/jackiechen/Apps/apache-maven-3.5.0/repository/mysql/mysql-connector-java/5.1.44/mysql-connector-java-5.1.44.jar"/>

    <context id="MysqlTables" targetRuntime="MyBatis3">

        <!--取消注释-->
        <commentGenerator>
            <property name="suppressDate" value="true"/>
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>

        <!--连接信息-->
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://172.19.216.49:3306/vote?useUnicode=true&amp;characterEncoding=utf8"
                        userId="keycloak"
                        password="admin">
        </jdbcConnection>

        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>

        <javaModelGenerator targetPackage="study.java.vote.smile.model" targetProject="src/main/java">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>

        <sqlMapGenerator targetPackage="study.java.vote.smile.dao" targetProject="src/main/resources">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>

        <javaClientGenerator type="XMLMAPPER" targetPackage="study.java.vote.smile.dao"
                             targetProject="src/main/java">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>

        <table tableName="user_info" domainObjectName="UserInfo"
            enableInsert="true" enableSelectByExample="false" enableUpdateByExample="false"
            enableDeleteByExample="false" enableCountByExample="false" enableUpdateByPrimaryKey="false">
            <!--useActualColumnNames 若为true，则采用列名作为实体属性名；为false则会将名称转为驼峰形式-->
            <property name="useActualColumnNames" value="false"/>
            <!--用来指定自动生成主键的属性，MBG在生成insert的SQL映射文件中插入一个<selectKey>元素，其包含两个必选属性
                column：列名
                sqlStatement:返回新值的Sql语句，预定义值：Cloudscape DB2 DB2_MF Derby HSQLDB Informix MySql SqlServer SYBASE JDBC-->
            <generatedKey column="ID" sqlStatement="MySql"/>
            <!--用来重写列，MBG会将指定的column的值填充为java bean中property的值，其包含一个必填属性
                column：需要重写的列
                property[非必填]：要使用的java属性的名称（若不填，则默认为同名或同名驼峰形式。此时该项完全可以不配置）-->
            <!--<columnOverride column="DATE_FIELD" property="startDate" />-->
            <!--<columnOverride column="LONG_VARCHAR_FIELD" jdbcType="VARCHAR" />-->
            <!--屏蔽无需生成的列-->
            <!--<ignoreColumn column="FRED" />-->
        </table>
    </context>
</generatorConfiguration>